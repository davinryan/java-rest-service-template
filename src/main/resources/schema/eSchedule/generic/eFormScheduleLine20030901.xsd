<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:element name="eFormScheduleLine">
		<xsd:annotation>
			<xsd:documentation>The eForm schedule line details</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
			
				<xsd:element name="schedule_line_number">
					<xsd:annotation>
						<xsd:documentation>A derived sequential input line number for the schedule.  Format Decimal 3</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:integer">
							<xsd:totalDigits value="3"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				
				<xsd:element name="service_date" type="xsd:dateTime">
					<xsd:annotation>
						<xsd:documentation>The date the provider performed the treatment.  Format DateTime</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				
				<xsd:element name="facility_code" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>The facility code associated to the schedule line  Format A5</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="6"/>
							<xsd:minLength value="0"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				
				<xsd:element name="loading_code" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>The loading code associated to the schedule line  Format A5</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="5"/>
							<xsd:minLength value="0"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				
				<xsd:element name="claimed_total_amount">
					<xsd:annotation>
						<xsd:documentation>The schedule line total amount claimed. The total amount claimed must be positive as credits are not allowed.  Format Decimal 8.2</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:decimal">
							<xsd:totalDigits value="8"/>
							<xsd:fractionDigits value="2"/>
							<xsd:minInclusive value="+0.00"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				
				<xsd:element name="claimed_hours" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>The schedule line total hours claimed  Format Numeric 2</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:nonNegativeInteger">
							<xsd:totalDigits value="2"/>
							<xsd:fractionDigits value="0"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				
				<xsd:element name="claimed_minutes" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>The schedule line total minutes claimed  Format Numeric 2</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:nonNegativeInteger">
							<xsd:totalDigits value="2"/>
							<xsd:fractionDigits value="0"/>
							<xsd:minInclusive value="00"/>
							<xsd:maxInclusive value="59"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				
				<xsd:element name="claimed_distance_travelled" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>The schedule line total distance travelled claimed  Format Numeric</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:nonNegativeInteger">
							<xsd:totalDigits value="5"/>
							<xsd:fractionDigits value="0"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				
				<xsd:element name="claimed_units" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>The schedule line units claimed Format Numeric 5 + 2</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:decimal">
							<xsd:totalDigits value="7"/>
							<xsd:fractionDigits value="2"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				
				<xsd:element name="schedule_line_comment" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>A free form schedule line comment  Format VA255</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="255"/>
							<xsd:minLength value="0"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
