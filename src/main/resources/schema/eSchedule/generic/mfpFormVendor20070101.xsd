<xsd:schema xmlns:accdt="http://xmlschema.acc.co.nz/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:include schemaLocation="../type/ACCDataType.xsd"/>
	<xsd:element name="mfpFormVendor">
		<xsd:annotation>
			<xsd:documentation>The eForm basic vendor details</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="vendor_name">
					<xsd:annotation>
						<xsd:documentation>This input field is for validation purposes only  Format  VA60.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="60"/>
							<xsd:minLength value="1"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="vendor_acc_identifier" type="VendorACCIdentifier">
					<xsd:annotation>
						<xsd:documentation>This is the vendor acc identifier  Format  VA12.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="vendor_gst_number" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>The gst number of the vendor  Format A11. A pattern was added to this element on 16-6-2003 to validate data as being 0 thru 9  '-' and ' ' only</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="11"/>
							<xsd:minLength value="0"/>
							<xsd:pattern value="[0-9 -]*"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="vendor_phone_number" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>This is an input phone number string that is only used for visual validation purposes.  Format VA15</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="15"/>
							<xsd:minLength value="0"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
